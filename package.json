{
  "name": "status-range-handler",
  "version": "0.3.0",
  "description": "Allows a user to handle error responses easily and quickly by categorizing standard responses by range",
  "type": "module",
  "main": "cjs/index.js",
  "exports": {
    "require": "./cjs/index.js",
    "import": "./esm/index.js"
  },
  "scripts": {
    "build": "tsc && tsc --module commonjs --outdir cjs/ && npm run build-packages",
    "build-packages": "echo '{\"type\": \"module\"}' > esm/package.json && echo '{\"type\": \"commonjs\"}' > cjs/package.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "status",
    "status_handling",
    "handling",
    "defaults",
    "request",
    "request_handling",
    "requests"
  ],
  "author": "Colin Brennan",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
